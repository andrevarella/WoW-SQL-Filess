				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parentDecrementButton" />
					<Anchor point="BOTTOMRIGHT" relativeTo="$parentIncrementButton" />
				</Anchors>
				<Scripts>
					<OnMouseWheel>
						if self:GetParent():IsEnabled() then
						local amount = IsShiftKeyDown() and 10 or 1;
						if delta > 0 then
						NumericInputSpinner_Increment(self:GetParent(), amount)
						else
						NumericInputSpinner_Decrement(self:GetParent(), amount)
						end
						self:GetParent():ClearFocus();
						end
					</OnMouseWheel>
				</Scripts>
			</Frame>
