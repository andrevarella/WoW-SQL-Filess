<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	
	
	<Frame name="ACDFrame" hidden="false" parent="UIParent" movable="true">
		<Size>
			<AbsDimension x="0" y="0"/>
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT" relativePoint="TOPLEFT">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		
		
		<Scripts>
			<OnLoad>
				ACD_OnLoad();
			</OnLoad>
			<OnUpdate>
				ACD_OnUpdate(arg1);
			</OnUpdate>
			<OnEvent>
				ACD_OnEvent(event);
			</OnEvent>
		</Scripts>
	</Frame>
	

	<Frame name="ACDNumFrame" hidden="false" parent="UIParent" movable="true">
		<Size>
			<AbsDimension x="128" y="128"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER">
				<Offset>
					<AbsDimension x="0" y="128"/>
				</Offset>
			</Anchor>
		</Anchors>
				
		<Scripts>
			
		</Scripts>
		<Layers>
			<Layer level="HIGH">			
				<Texture name="ACDNumTens" file="Interface\Addons\ArenaCountDown\Artwork\6">					
					<Size>
						<AbsDimension x="64" y="128"/>
					</Size>
					<Anchors>
						<Anchor point="RIGHT" relativePoint="CENTER">
							<Offset>
								<AbsDimension x="20" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>	

				<Texture name="ACDNumOnes" file="Interface\Addons\ArenaCountDown\Artwork\0">					
					<Size>
						<AbsDimension x="64" y="128"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativePoint="CENTER">
							<Offset>
								<AbsDimension x="-20" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>		

				<Texture name="ACDNumOne" file="Interface\Addons\ArenaCountDown\Artwork\0">					
					<Size>
						<AbsDimension x="64" y="128"/>
					</Size>
					<Anchors>
						<Anchor point="CENTER" relativePoint="CENTER">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>								
				
				<Texture name="ACDLarge" file="Interface\Addons\ArenaCountDown\Artwork\fight">					
					<Size>
						<AbsDimension x="128" y="64"/>
					</Size>
					<Anchors>
						<Anchor point="CENTER" relativePoint="CENTER">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>				
				
			</Layer>			
		</Layers>	
	</Frame>

